{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "concurrently \"npm run dev --workspace=apps/web\" \"npm run dev --workspace=apps/docs\" --names \"web,docs\" --prefix-colors \"blue,green\"",
    "start": "npm run start --workspaces",
    "lint": "npm run lint --workspaces",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "npm run check-types --workspaces"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "concurrently": "^9.2.1",
    "eslint-config-next": "^16.0.1",
    "prettier": "^3.6.2",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
